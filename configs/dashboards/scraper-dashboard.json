{
  "dashboard": {
    "title": "PC Scraper - Overview",
    "tags": [
      "scraper",
      "rabbitmq",
      "postgres"
    ],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Total Products Scraped",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(scraper_products_scraped_total)",
            "legendFormat": "Products"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Messages Processed",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(consumer_messages_processed_total)",
            "legendFormat": "Messages"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Success Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "sum(consumer_messages_processed_total{status='success'}) / sum(consumer_messages_processed_total) * 100"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Cloudflare Detections",
        "type": "stat",
        "targets": [
          {
            "expr": "scraper_cloudflare_detections_total"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Products Scraped by Category",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(scraper_products_scraped_total[5m])",
            "legendFormat": "{{category}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        }
      },
      {
        "id": 6,
        "title": "Scraping Duration (p95)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(scraper_page_duration_seconds_bucket[5m]))",
            "legendFormat": "{{category}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        }
      },
      {
        "id": 7,
        "title": "Database Insert Status",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by(status) (consumer_database_inserts_total)",
            "legendFormat": "{{status}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 8,
        "title": "Duplicates Skipped",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(scraper_duplicates_skipped_total[5m])",
            "legendFormat": "{{category}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 12
        }
      },
      {
        "id": 9,
        "title": "Message Processing Duration (avg)",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(consumer_message_processing_duration_seconds_sum[5m]) / rate(consumer_message_processing_duration_seconds_count[5m])",
            "legendFormat": "Seconds"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 12
        }
      }
    ]
  }
}